meta {
  name: Get Care Types
  type: http
  seq: 11
}

get {
  url: {{GATEWAY_URL}}/api/announcements/care-types
  body: none
  auth: bearer
}

auth:bearer {
  token: {{ACCESS_TOKEN}}
}

script:post-response {
  if (res.status === 200) {
    console.log(`✅ Retrieved ${Array.isArray(res.body) ? res.body.length : 0} care types`);
  } else {
    console.error('❌ Failed to fetch care types:', res.status);
  }
}

